{
  "private": true,
  "license": "WTFPL",
  "type": "module",
  "scripts": {
    "dev": "vite --open --host",
    "dev:tsc": "tsc --noEmit --watch",
    "build": "npm run lint:scss && npm run lint:ts && npm run lint:html && tsc && vite build",
    "lint:ts": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:scss": "prettier --write \"./src/**/*.scss\" && stylelint \"./src/**/*.scss\" --custom-syntax postcss-scss",
    "lint:html": "prettier --write \"./**/*.html\"",
    "preview": "npm run build && vite preview --open",
    "deploy": "npm run build && gh-pages -d \"./dist\""
  },
  "dependencies": {
    "i18next": "^23.5.1",
    "i18next-browser-languagedetector": "^7.1.0",
    "i18next-http-backend": "^2.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.2.2",
    "react-select": "^5.7.4",
    "react-tooltip": "^5.21.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "autoprefixer": "^10.4.15",
    "browserslist-to-esbuild": "^1.2.0",
    "eslint": "^8.49.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "gh-pages": "^6.0.0",
    "postcss": "^8.4.29",
    "postcss-scss": "^4.0.8",
    "prettier": "^3.0.3",
    "sass": "^1.67.0",
    "sharp": "^0.32.5",
    "stylelint": "^15.10.3",
    "stylelint-config-recommended-scss": "^13.0.0",
    "stylelint-scss": "^5.1.0",
    "svgo": "^3.0.2",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-image-optimizer": "^1.1.7"
  }
}
